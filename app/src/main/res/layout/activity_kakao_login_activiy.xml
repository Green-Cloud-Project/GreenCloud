    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".kakaologin.KakaoLoginActiviy">

    <data>
        <variable
            name="kakaoLogin"
            type="com.share.greencloud.kakaologin.KakaoLoginActiviy" />
    </data>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.kakao.usermgmt.LoginButton
